
<li>
<div class="row">
  <div class="col s3">
    <div class="card medium">
      <div class="card-image">
        <img src={{book.image}} alt={{book.title}}>
      </div>
      <div class="card-content">
        {{book.title}}<br>
        by: {{book.author}}<br>
        ${{book.price}}<br>
      </div>
      <div class="card-action">
        <button class="waves-effect waves-light btn" {{ action 'delete' book}}>Delete</button>
      </div>
    </div>
  </div>
</div>
</li>
